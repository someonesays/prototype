{
  "name": "@/client",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "wrangler pages dev build --port 3000",
    "dev": "vite dev --mode development",
    "dev/staging": "vite dev --mode staging",
    "build": "bun utils/copyHeaders.ts && vite build && bun utils/deleteHeaders.ts",
    "build/staging": "NODE_ENV=staging bun utils/copyHeaders.ts && vite build --mode staging && bun utils/deleteHeaders.ts",
    "build/dev": "NODE_ENV=development bun utils/copyHeaders.ts && vite build --mode development && bun utils/deleteHeaders.ts",
    "preview": "vite preview",
    "format": "cd ../../ && bun format"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.13.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "svelte": "^5.15.0",
    "svelte-turnstile": "^0.9.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "wrangler": "^3.99.0",
    "zod": "^3.24.1"
  },
  "dependencies": {
    "@/public": "workspace:*",
    "@discord/embedded-app-sdk": "workspace:*",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@sveltejs/adapter-static": "^3.0.6",
    "eventemitter3": "^5.0.1",
    "focus-trap": "^7.6.2"
  }
}
